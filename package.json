{
  "name": "nestjs-adjaas-api-producer",
  "version": "1.0.4",
  "description": "NestJS adjaas API producer",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nodemon",
    "start:dev": "nodemon --watch",
    "start:debug": "nodemon --config nodemon-debug.json",
    "lint": "eslint --ext .ts,.js src",
    "test:unit": "jest",
    "test": "npm run test:unit && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.2.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "csv-parser": "3.0.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-winston": "4.2.0",
    "mssql": "^6.2.1",
    "mysql": "^2.18.1",
    "node-vault": "0.9.22",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "sonar-scanner": "3.1.0",
    "sonarqube-scanner": "2.8.1",
    "swagger-stats": "0.95.16",
    "swagger-ui-express": "4.5.0",
    "typeorm": "^0.3.11",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-angular": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@cucumber/cucumber": "8.7.0",
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "8.0.3",
    "@nestjs/testing": "^8.2.3",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/commit-analyzer": "6.3.3",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/gitlab": "^4.0.4",
    "@semantic-release/npm": "5.3.4",
    "@semantic-release/release-notes-generator": "7.3.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/multer": "1.4.7",
    "@types/node": "^14.14.41",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "concurrently": "5.0.0",
    "eslint": "6.7.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "nock": "11.7.0",
    "nodemon": "2.0.1",
    "prettier": "1.19.1",
    "semantic-release": "15.13.31",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged",
      "pre-push": "npm run test:cov && npm run test:e2e",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{json,md}": [
      "prettier --write"
    ],
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branch": "master",
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/gitlab",
        {
          "gitlabUrl": "https://apps.bsc.aws.societegenerale.com/gitlab"
        }
      ]
    ]
  }
}
